@article{ WOS:000794519200001,
Author = {Rodriguez-Mendez, Ivan and Luke, Jonas Philipp and Rosa Gonzalez,
   Fernando Luis},
Title = {MASE: An Instrument Designed to Record Underwater Soundscape},
